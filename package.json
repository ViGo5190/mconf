{
  "name": "mconf",
  "version": "0.4.0",
  "description": "Micro config for nodejs projects.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha test/ --recursive --reporter spec",
    "cover": "istanbul cover -- _mocha test/ --recursive --reporter spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "babel -d dist src --source-maps",
    "build:watch": "babel -w -d dist src --source-maps",
    "postpublish": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViGo5190/mconf.git"
  },
  "keywords": [
    "configuration",
    "environment",
    "env",
    "config-node",
    "node-config",
    "configuration",
    "config",
    "conf"
  ],
  "author": "Stanislav Gumeniuk <i@vigo.su>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViGo5190/mconf/issues"
  },
  "homepage": "https://github.com/ViGo5190/mconf#readme",
  "dependencies": {
    "extend": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mockery": "^1.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "webpack": "^1.13.2"
  }
}
